{
  "todo": {
    "prefix": "todo",
    "body": ["// TODO(josh): ${1}"]
  },
  "fixme": {
    "prefix": "fixme",
    "body": ["// FIXME(josh): ${1}"]
  },
  "note": {
    "prefix": "note",
    "body": ["// NOTE(josh): ${1}"]
  },
  "doc": {
    "prefix": "doc",
    "body": ["/// <summary>\n///    ${1}\n///</summary>"]
  }
}
